    "body-parser": "^1.20.1",
    "co-body": "^6.1.0",
    "concurrently": "^7.6.0",
    "core-js": "^3.8.3",
    "cross-env": "^7.0.3",
    "dotenv": "^16.3.1",
    "echarts": "^5.4.1",
    "element-ui": "^2.15.12",
    "error-stack-parser": "^2.1.4",
    "express": "^4.18.2",
    "express-fileupload": "^1.4.3",
    "js-base64": "^3.7.3",
    "less": "^4.1.3",
    "less-loader": "^11.1.0",
    "pako": "^2.1.0",
